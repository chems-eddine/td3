public class PointNom extends PointA{
private String Nom;
public PointNom(double abscisse, double ordonnée, String Nom){
super(abscisse,ordonnée);
this.Nom=Nom;
}
 public String getNom()
{ return this.Nom;}


public void AfficheCoord(){
System.out.println("le point "+getNom()+super.AfficheCoord());
 } }
 
